        <class-b>com.eventpool.common.dto.EventSettingsDTO</class-b>
			 <field><a>eventId               </a><b>eventId               </b></field>
			<field><a>showGoogleMap         </a><b>showGoogleMap         </b></field>
			<field><a>showOrganizerName     </a><b>showOrganizerName     </b></field>
			<field><a>showOrganizerDesc     </a><b>showOrganizerDesc     </b></field>
			<field><a>showContactDetails    </a><b>showContactDetails    </b></field>
