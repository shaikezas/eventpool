			<field><a>eventUrl      </a><b>eventUrl     </b></field>
			<field><a>eventWebSiteUrl      </a><b>evenWebSiteUrl     </b></field>
    </mapping>

   <mapping stop-on-errors="false" wildcard="false">
        <class-a>com.eventpool.common.entities.Ticket</class-a>
        <class-b>com.eventpool.common.dto.TicketDTO</class-b>
        	<field><a>id</a><b>id</b></field>
  			<field><a>name        </a><b>name        </b></field>
			<field><a>description </a><b>description </b></field>
			<field><a>saleStart    </a><b>saleStart   </b></field>
